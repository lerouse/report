
{
  "name": "mblsolutions/report",
  "description": "MBL Solutions Laravel Reports",
  "keywords": ["mblsolutions", "report", "laravel"],
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "MBL Solutions Ltd.",
      "email": "support@mblsolutions.co.uk"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "illuminate/console": "^6.0|^7.0|^8.0",
    "illuminate/events": "^6.0|^7.0|^8.0",
    "illuminate/database": "^6.0|^7.0|^8.0",
    "illuminate/queue": "^6.0|^7.0|^8.0",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "illuminate/http": "^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "MBLSolutions\\Report\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MBLSolutions\\Report\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "MBLSolutions\\Report\\ReportServiceProvider"
      ]
    }
  }
}