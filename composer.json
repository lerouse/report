{
  "name": "mblsolutions/report",
  "description": "Laravel Report package",
  "keywords": ["redu", "report", "laravel"],
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "MBL Solutions Ltd.",
      "email": "support@mblsolutions.co.uk"
    }
  ],
  "require": {
    "php": "^8.0|^8.1",
    "ext-json": "*",
    "illuminate/cache": "^8.0",
    "illuminate/console": "^8.0",
    "illuminate/database": "^7.30.5|^8.0",
    "illuminate/events": "^8.0",
    "illuminate/filesystem": "^8.0",
    "illuminate/queue": "^8.0",
    "illuminate/support": "^8.0",
    "illuminate/http": "^8.0",
    "maatwebsite/excel": "^3.1",
    "lerouse/laravel-repository": "^2.0",
    "laravel/legacy-factories": "^1.2"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "MBLSolutions\\Report\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MBLSolutions\\Report\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "MBLSolutions\\Report\\ReportServiceProvider"
      ]
    }
  }
}
